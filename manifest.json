{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Surround Sound X",
  "version": "1",
  "permissions": [
    "tabCapture",
    "activeTab"
  ],
 "background":{
    "scripts": ["background.js"]
   },
   "icons": {
          "128": "sound.png" },
  "browser_action": {
    "default_title": "Surround Sound X",
    "default_popup": "popup.html"
  }
}
